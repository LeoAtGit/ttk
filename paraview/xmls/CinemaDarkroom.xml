<?xml version="1.0" encoding="UTF-8"?>
<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="ttkCinemaDarkroomCamera" class="ttkCinemaDarkroomCamera" label="TTK Darkroom Camera">
      <Documentation long_help="Cinema Darkroom Compositing" short_help="Cinema Darkroom Compositing">
        This source generates a single vertex with point data to represent a camera that can be used for Cinema Darkroom rendering. The source can also be synchronized with the camera of an active view port.
      </Documentation>

      <InputProperty name="Position" port_index="0" command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataObject" />
        </DataTypeDomain>
        <Documentation>vtkDataObject TODO.</Documentation>
        <Hints>
          <Optional />
        </Hints>
      </InputProperty>

      <InputProperty name="Near Far Bounding Box" port_index="1" command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataObject" />
        </DataTypeDomain>
        <Documentation>vtkDataObject TODO.</Documentation>
        <Hints>
          <Optional />
        </Hints>
      </InputProperty>

      <InputProperty name="Focus Object" port_index="2" command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataObject" />
        </DataTypeDomain>
        <Documentation>vtkDataObject TODO.</Documentation>
        <Hints>
          <Optional />
        </Hints>
      </InputProperty>

      <IntVectorProperty name="Resolution" command="SetResolution" number_of_elements="2" default_values="256 256">
        <Documentation>Pixel Resolution of the Camera.</Documentation>
      </IntVectorProperty>
      <DoubleVectorProperty name="PositionVec" label="Position" command="SetPosition" number_of_elements="3" default_values="0 0 0">
        <Documentation>Camera Position.</Documentation>
        <Hints>
          <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="Position" value="null"/>
        </Hints>
      </DoubleVectorProperty>
      <DoubleVectorProperty name="Up" command="SetUp" number_of_elements="3" default_values="0 1 0">
        <Documentation>Camera Up-Vector.</Documentation>
      </DoubleVectorProperty>
      <DoubleVectorProperty name="Direction" command="SetDirection" number_of_elements="3" default_values="0 0 -1">
        <Documentation>Camera Direction.</Documentation>
        <Hints>
          <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="Focus Object" value="null"/>
        </Hints>
      </DoubleVectorProperty>

      <DoubleVectorProperty name="NearFar" command="SetNearFar" number_of_elements="2" default_values="0.01 1">
        <Documentation>Camera Near Far.</Documentation>
        <Hints>
          <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="Near Far Bounding Box" value="null"/>
        </Hints>
      </DoubleVectorProperty>

      <IntVectorProperty name="Orthographic" command="SetOrthographic" number_of_elements="1" default_values="1">
        <BooleanDomain name="bool" />
        <Documentation>Use Orthographic Projection.</Documentation>
      </IntVectorProperty>
      <DoubleVectorProperty name="Angle" command="SetAngle" number_of_elements="1" default_values="90">
        <Documentation>Camera View Angle (Perspective Projection Only).</Documentation>
        <Hints>
          <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="Orthographic" value="0" />
        </Hints>
      </DoubleVectorProperty>
      <DoubleVectorProperty name="Scale" command="SetScale" number_of_elements="1" default_values="1">
        <Documentation>Camera Scale Factor (Orthographic Projection Only).</Documentation>
        <Hints>
          <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="Orthographic" value="1" />
        </Hints>
      </DoubleVectorProperty>

      <Property name="SyncParaViewCamera" label="Sync ParaView Camera" command="SyncParaViewCamera" panel_widget="command_button">
        <Documentation>Synchronize the active ParaView camera with the active Darkroom camera.</Documentation>
      </Property>
      <Property name="SyncDarkroomCamera" command="SyncDarkroomCamera" panel_widget="command_button">
        <Documentation>Synchronize the active Darkroom camera with the active ParaView camera.</Documentation>
      </Property>

      <OutputPort name="Camera" index="0" id="port0"/>
      <OutputPort name="Frustum" index="1" id="port1"/>

      ${DEBUG_WIDGETS}

      <Hints>
        <ShowInMenu category="TTK" />
      </Hints>
    </SourceProxy>

  </ProxyGroup>

  <!-- ////////////////////////////////////////////////////////////////////-->

  <ProxyGroup name="filters">

    <!-- ////////////////////////////////////////////////////////////////////-->

    <SourceProxy name="ttkCinemaDarkroomCompositing" class="ttkCinemaDarkroomCompositing" label="TTK Darkroom Compositing">
      <Documentation long_help="Cinema Darkroom Compositing" short_help="Cinema Darkroom Compositing">
        This filter composists all input vtkImageData objects at its first port based on a depth buffer.

        "Cinema Database Specification - Dietrich Release v1.2".
        D. Rogers, J. Woodring, J. Ahrens, J. Patchett, and J. Lukasczyk.
        Technical Report LA-UR-17-25072, Los Alamos National Laboratory. 2018.

        "Cinema Darkroom: A Deferred Rendering Framework for Large-Scale Datasets".
        J. Lukasczyk, C. Garth, M. Larsen, W. Engelke, I. Hotz, D. Rogers, J. Ahrens, and R. Maciejewski.
        IEEE 10th Symposium on Large Data Analysis and Visualization (LDAV). 2020.
      </Documentation>

      <InputProperty name="Input" port_index="0" clean_command="RemoveAllInputs" command="AddInputConnection" multiple_input="1">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData" />
          <DataType value="vtkMultiBlockDataSet" />
        </DataTypeDomain>
        <InputArrayDomain name="input_array" attribute_type="point" />
        <Documentation>vtkImageData objects that are going to be composed into one image based on a depth buffer.</Documentation>
      </InputProperty>

      <StringVectorProperty name="Depth" command="SetInputArrayToProcess" element_types="0 0 0 0 2" number_of_elements="5" default_values="0">
        <ArrayListDomain attribute_type="Scalars" name="array_list">
          <RequiredProperties>
            <Property function="Input" name="Input" />
          </RequiredProperties>
        </ArrayListDomain>
        <Documentation>The array that contains depth values.</Documentation>
      </StringVectorProperty>

      ${DEBUG_WIDGETS}

      <Hints>
        <ShowInMenu category="TTK - Cinema" />
      </Hints>
    </SourceProxy>

    <!-- ////////////////////////////////////////////////////////////////////-->

    <SourceProxy name="ttkCinemaDarkroomColorMapping" class="ttkCinemaDarkroomColorMapping" label="TTK Darkroom ColorMapping">
      <Documentation long_help="Cinema Darkroom Color Mapping" short_help="Cinema Darkroom Color Mapping">
        This filter maps scalars to either a single color, a manually defined color map, or any predefined color map.

        "Cinema Database Specification - Dietrich Release v1.2".
        D. Rogers, J. Woodring, J. Ahrens, J. Patchett, and J. Lukasczyk.
        Technical Report LA-UR-17-25072, Los Alamos National Laboratory. 2018.

        "Cinema Darkroom: A Deferred Rendering Framework for Large-Scale Datasets".
        J. Lukasczyk, C. Garth, M. Larsen, W. Engelke, I. Hotz, D. Rogers, J. Ahrens, and R. Maciejewski.
        IEEE 10th Symposium on Large Data Analysis and Visualization (LDAV). 2020.
      </Documentation>

      <InputProperty name="Input" command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData" />
        </DataTypeDomain>
        <InputArrayDomain name="input_array" attribute_type="point" />
        <Documentation>A vtkImageData object.</Documentation>
      </InputProperty>

      <StringVectorProperty name="Scalars" command="SetInputArrayToProcess" element_types="0 0 0 0 2" number_of_elements="5" default_values="0">
        <ArrayListDomain attribute_type="Scalars" name="array_list">
          <RequiredProperties>
            <Property function="Input" name="Input" />
          </RequiredProperties>
        </ArrayListDomain>
        <Documentation>The array that contains the scalar values that going to be mapped to color.</Documentation>
      </StringVectorProperty>

      ${DARKROOM_COLOR_MAPPING_WIDGETS}

      ${DEBUG_WIDGETS}

      <Hints>
        <ShowInMenu category="TTK - Cinema" />
      </Hints>
    </SourceProxy>

